/** @file time.S
 *
 * @brief time sycall wrapper
 *
 * @author Kartik Subramanian <ksubrama@andrew.cmu.edu>
 * @date 2008-10-31
 */

#include <asm.h>
#include <bits/swi.h>

	.file "task_create.S"
	.global errno
FUNC(task_create)
	ldr	r3, =errno
	swi	CREATE_SWI
	cmp	r0, #0
	bge	ret
	neg	r1, r0
	str	r1, [r3]
	mov	r0, #-1
ret:
	mov	pc, lr
